<Window x:Class="Benchmark_Multi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Benchmark_Multi"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Click="Button_Click" Margin="2" Width="70" Height="35">Start Bench</Button>
            <Label x:Name="lbl_message" Margin="100, 0, 100, 0">Press Button To Start</Label>
            <Label x:Name="lbl_result" Margin="100, 0, 100, 0"></Label>
        </StackPanel>
    </Grid>
</Window>
